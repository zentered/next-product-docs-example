{
  "name": "next-product-docs-example",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zentered/next-product-docs-example.git"
  },
  "author": "Zentered (https://zentered.co)",
  "contributors": [
    "Patrick Heneise (https://github.com/PatrickHeneise)",
    "Moritz Sattler (https://github.com/MoSattler)"
  ],
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "unlink-component": "cd ../next-product-docs && npm unlink && cd ./node_modules/react && npm unlink && cd ../react-dom && npm unlink",
    "create-links": "cd ../next-product-docs && npm link && cd ./node_modules/react && npm link && cd ../react-dom && npm link && cd ../../../next-product-docs-example",
    "link-component-to-folder": "npm link react && npm link react-dom && npm link @opstrace/next-product-docs",
    "link-docs-component": "npm run create-links && npm run link-component-to-folder"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{jsx}": [
      "stylelint"
    ],
    "*.{js,json,jsx,md,mdx}": [
      "prettier --write"
    ],
    "*.{js,jsx,md,mdx}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^1.0.1",
    "next": "10.2.2",
    "next-mdx-remote": "^3.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scroll": "^1.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-markdown": "^2.1.0",
    "eslint-plugin-mdx": "^1.13.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.0",
    "prettier": "^2.3.0",
    "sass": "^1.34.0",
    "tailwindcss": "^2.1.2"
  }
}
